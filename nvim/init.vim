syntax on
set number
set nocompatible
set encoding=UTF-8
set termguicolors

set softtabstop=4
set tabstop=4
set shiftwidth=4
set expandtab


let g:transparent_enabled = v:true

source $HOME/.config/nvim/plugins/plugins.vim
source $HOME/.config/nvim/keybinds.vim

colorscheme decay    "gruvbox tokyonight
